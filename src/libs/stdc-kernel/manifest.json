{
    "$schema": "https://schemas.cute.engineering/stable/cutekit.manifest.component.v1",
    "id": "stdc-kernel",
    "description": "Libc compatibility layer for the kernel (useful for data structures)",
    "type": "lib",
    "requires": [
        "mu-mem"
    ],
    "enableIf": {
        "sys": [
            "mu"
        ]
    },
    "provides": [
        "stdc-ansi"
    ],
    "props": {
        "cpp-root-include": true
    }
}